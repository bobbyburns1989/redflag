<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>CFBundleDevelopmentRegion</key>
	<string>$(DEVELOPMENT_LANGUAGE)</string>
	<key>CFBundleDisplayName</key>
	<string>Pink Flag</string>
	<key>CFBundleExecutable</key>
	<string>$(EXECUTABLE_NAME)</string>
	<key>CFBundleIdentifier</key>
	<string>$(PRODUCT_BUNDLE_IDENTIFIER)</string>
	<key>CFBundleInfoDictionaryVersion</key>
	<string>6.0</string>
	<key>CFBundleName</key>
	<string>Pink Flag</string>
	<key>CFBundlePackageType</key>
	<string>APPL</string>
	<key>CFBundleShortVersionString</key>
	<string>$(FLUTTER_BUILD_NAME)</string>
	<key>CFBundleSignature</key>
	<string>????</string>
	<key>CFBundleVersion</key>
	<string>$(FLUTTER_BUILD_NUMBER)</string>
	<key>LSRequiresIPhoneOS</key>
	<true/>
	<key>UIDeviceFamily</key>
	<array>
		<integer>1</integer>
	</array>
	<key>UILaunchStoryboardName</key>
	<string>LaunchScreen</string>
	<key>UIMainStoryboardFile</key>
	<string>Main</string>
	<key>UISupportedInterfaceOrientations</key>
	<array>
		<string>UIInterfaceOrientationPortrait</string>
		<string>UIInterfaceOrientationLandscapeLeft</string>
		<string>UIInterfaceOrientationLandscapeRight</string>
	</array>
	<key>CADisableMinimumFrameDurationOnPhone</key>
	<true/>
	<key>UIApplicationSupportsIndirectInputEvents</key>
	<true/>
	<!-- URL schemes for emergency calling -->
	<key>LSApplicationQueriesSchemes</key>
	<array>
		<string>tel</string>
	</array>
	<!-- Allow HTTP connections for development (use HTTPS in production) -->
	<key>NSAppTransportSecurity</key>
	<dict>
		<key>NSAllowsArbitraryLoads</key>
		<true/>
	</dict>
	<!-- Privacy Permission Descriptions (Required by Apple) -->
	<key>NSPhotoLibraryUsageDescription</key>
	<string>Pink Flag needs access to your photo library to select images for reverse image search to help detect potential catfishing or fake profiles.</string>
	<key>NSCameraUsageDescription</key>
	<string>Pink Flag needs camera access to take photos for reverse image search to help detect potential catfishing or fake profiles.</string>
	<key>NSLocationWhenInUseUsageDescription</key>
	<string>Pink Flag does not access your location for privacy reasons.</string>
	<key>NSLocationAlwaysAndWhenInUseUsageDescription</key>
	<string>Pink Flag does not access your location for privacy reasons.</string>
	<key>NSMicrophoneUsageDescription</key>
	<string>Pink Flag does not access your microphone.</string>
	<key>NSContactsUsageDescription</key>
	<string>Pink Flag does not access your contacts.</string>
	<!-- Emergency calling permission -->
	<key>NSPhoneCallUsageDescription</key>
	<string>Pink Flag uses this to connect you to emergency services and safety hotlines.</string>
</dict>
</plist>
